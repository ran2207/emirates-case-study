  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "23.6.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
